{
  "name": "barquo",
  "version": "0.1.0",
  "description": "Library for reading and decoding barcodes",
  "repository": {
    "type": "git",
    "url": "https://github.com/bromzh/barquo.git"
  },
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build": "npm run tsc && npm run cpx:package",
    "ci": "concurrently \"npm run tsc:w\" \"npm run test:ci\"",
    "tsc": "tsc -p src",
    "tsc:w": "tsc -p src -w",
    "test": "karma start",
    "test:ci": "karma start --reporters dots --log-level disable --no-single-run --auto-watch",
    "cpx:package": "cpx package.json lib",
    "lint": "tslint src/**/*.ts",
    "watch": "concurrently \"npm run tsc:w\" \"npm run cpx:src:w\""
  },
  "keywords": [
    "barcode",
    "qrcode",
    "decoder"
  ],
  "author": "bromzn@gmail.com",
  "license": "WTFPL",
  "devDependencies": {
    "@types/jasmine": "^2.5.38",
    "awesome-typescript-loader": "~3.0.0-beta.5",
    "concurrently": "^3.1.0",
    "cpx": "^1.5.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "istanbul-instrumenter-loader": "^1.1.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "~0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "source-map-loader": "^0.1.5",
    "ts-helpers": "^1.1.2",
    "tslint": "~4.0.2",
    "typescript": "~2.1.1",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "~2.1.0-beta.11"
  }
}
